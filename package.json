{
  "name": "micro-websocket-experiment",
  "version": "0.1.0",
  "description": "Websocket microservice experiment using generators",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "bin"
  ],
  "greenkeeper": {
    "emails": false
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "mkdir -p dist && async-to-gen lib/index.js > dist/index.js",
    "example:echo": "node ./examples/echo.js",
    "example:greeter": "node ./examples/greeter.js",
    "wscat": "wscat -c ws://localhost:3000"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "rules": {
      "max-lines": 0,
      "ava/no-ignored-test-files": 0,
      "no-labels": 0,
      "no-unused-labels": 0,
      "no-unused-expressions": 0,
      "yoda": 0,
      "no-negated-condition": 0
    }
  },
  "bin": {
    "micro-ws": "./bin/micro-ws"
  },
  "repository": "robertlong/micro-websocket-experiment",
  "keywords": [
    "micro",
    "server",
    "microservice",
    "serverless",
    "websocket"
  ],
  "author": {
    "name": "Robert Long"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robertlong/micro-websocket-experiment/issues"
  },
  "homepage": "https://github.com/robertlong/micro-websocket-experiment#readme",
  "devDependencies": {
    "wscat": "^1.0.1",
    "xo": "^0.17.0"
  },
  "dependencies": {
    "async-to-gen": "1.1.4",
    "minimist": "1.2.0",
    "ws": "^1.1.1"
  }
}
